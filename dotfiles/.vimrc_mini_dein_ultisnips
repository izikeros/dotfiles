
"dein Scripts-----------------------------
if &compatible
  set nocompatible               " Be iMproved
endif

" Required:
set runtimepath+=/home/izik/.vim/bundle/repos/github.com/Shougo/dein.vim

" Required:
if dein#load_state('/home/izik/.vim/bundle')
  call dein#begin('/home/izik/.vim/bundle')

  " Let dein manage dein
  " Required:
  call dein#add('/home/izik/.vim/bundle/repos/github.com/Shougo/dein.vim')

  " Add or remove your plugins here:
  "call dein#add('Shougo/neosnippet.vim')
  "call dein#add('Shougo/neosnippet-snippets')

  "" snipmate
  call dein#add('SirVer/ultisnips')
  call dein#add('honza/vim-snippets')

  " Required:
  call dein#end()
  call dein#save_state()
endif

" Required:
filetype plugin indent on
syntax enable

